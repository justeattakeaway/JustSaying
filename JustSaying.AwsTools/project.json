{
	"version": "5.0.0",
	"authors": ["JUST EAT plc"],
	"copyright": "JUST EAT plc",
	"title": "JustSaying",
	"description": "A light-weight message bus on top of AWS SNS and SQS",
	"buildOptions": {
		"warningsAsErrors": true,
		"platform": "anycpu",
		"languageVersion": "csharp6",
		"debugType": "portable"
	},
	"packOptions": {
		"owners": ["JUST EAT plc"],
		"summary": "SNS and SQS Messaging",
		"tags": [ "aws", "sns", "sqs" ],
		"repository": {
			"type": "git",
			"url": "https://github.com/justeat/JustSaying.git"
		},
	  "requireLicenseAcceptance": true,
		"licenseUrl": "https://github.com/justeat/JustSaying/blob/develop/LICENSE.md",
		"projectUrl": "https://github.com/justeat/JustSaying"
	},
	"configurations": {
		"Debug": {

		},
		"Release": {
			"buildOptions": {
				"optimize": true
			}
		}
	},
  "dependencies": {
    "AWSSDK.Core": "3.3.7.1",
    "AWSSDK.SimpleNotificationService": "3.3.0.6",
    "AWSSDK.SQS": "3.3.1.6",
    "JustSaying.Messaging": "5.0.0",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "System.Linq": "4.3.0",
        "System.Runtime.Serialization.Formatters": "4.3.0",
        "System.Reflection.Extensions": "4.3.0",
        "System.Threading.ThreadPool": "4.3.0",
        "System.Net.NetworkInformation": "4.3.0",
        "System.Net.WebHeaderCollection": "4.3.0"
      }
    },
    "net451": {
      "dependencies": {
      }
    }
  }
}
